@for (int j = 0; j < 5; j++)
    {
        if (j < (int)TempData["intKey"])
        {
        <i class="fa fa-star" id="@TempData["category"]-@j"></i>

    }
    else
    {
        <i class="fa fa-star-o" id="@TempData["category"]-@j"></i>
    }
}

