@for (int j = 0; j < 5; j++)
    {
        if (j < (int)TempData["intKey"])
        {
        <i class="fa fa-star goldStar" id="@TempData["category"]-@j" onclick=testFunction("@TempData["category"]-@j","@TempData["tour"]")></i>

        }
        else
        {
        <i class="fa fa-star-o" id="@TempData["category"]-@j" onclick=testFunction("@TempData["category"]-@j","@TempData["tour"]")></i>
        }
}

